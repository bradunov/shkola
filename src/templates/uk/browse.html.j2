    {{ elements.init_elements() }}
    {{ elements.standard_background(template_params) }}

    <meta name="page" content="question">

    <style>
      html, body {
        height: 100%;
        margin: 0;
      }
    </style>

  </head>

  <body style="overscroll-behavior: none">
    {{ elements.standard_body(template_params) }}

    <!-- MOBILE MENU START -->
    {% import 'menu.html.j2' as menu %}
