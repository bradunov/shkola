             style="display:inline-block; width: calc(100% - 16px); text-align: center; margin: 11px 8px 8px 8px; font-size: 18px;">

          {{template_params["question"]}}

        </div>

      </div>
    </div>
    <!-- QUESTION END -->

    {{ elements.next("question_onnext()", "NASTAVI") }}

