              transform="translate({{d3_x}} {{r_y_start}})" style="fill:#489cba"/>
        {% if t_w < d1_w %}
          <text id="text_1_{{name}}" class="stats__cls-6 sh_font" data-name="d1" transform="translate({{d1_t_x}} {{t_y_start}})">
              <tspan x="-3.48" y="0">{{d1}}</tspan>
          </text>
        {% endif %}
