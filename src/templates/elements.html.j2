{% macro start_popup(name, extra_style="", bg_height="100%", bg_top="0px", toggle=True) %}
  <script>
      // When the user clicks on div, open the popup
      function popup_toggle_{{name}}(new_state) {
          var popup = document.getElementById("popup_{{name}}");
          var popup_background = document.getElementById("popup_background_{{name}}");
          if (new_state == undefined) {
            if (popup.style.visibility == "hidden") { 
                popup.style.visibility = "visible";
                popup_background.style.visibility = "visible";
                popup_background.style.opacity = "0.8";
            } else {
                popup.style.visibility = "hidden";
                popup_background.style.visibility = "hidden";
                popup_background.style.opacity = "0.8";
            }
          } else {
            popup.style.visibility = new_state;
            popup_background.style.visibility = new_state;
            popup_background.style.opacity = "0.8";
          }
      }
  </script>

  <!-- POPUP START -->

  <span id='popup_background_{{name}}' style='visibility: hidden;
      position: fixed; z-index: 2; 
      top: {{bg_top}}; left: 0%; width: 100%; height: {{bg_height}}; 
      opacity=0.8; background-color: white; ' 
      {% if toggle %}
      onclick='popup_toggle_{{name}}()'
      {% endif %}
  >
  </span>        

  <div id='popup_{{name}}' style='visibility: hidden; position: fixed; z-index: 3; {{extra_style}}'>    

{% endmacro %}


{% macro end_popup() %}
  </div>
  <!-- POPUP END -->
{% endmacro %}






{% macro stats(name, d1, d2, d3, missed) %}

{% set r_x_start = 35 %}
{% set r_y_start = 151 %}
{% set t_y_start = 163 %}
{% set r_width = 265 %}
{% set r_height = 15 %}
{% set total = d1 + d2 + d3 + missed %}
{% if total == 0 %}
  {% set total = 1 %}
{% endif %}
{% set d1_w = (r_width * d1 / total)|round|int %}
{% set d2_w = (r_width * d2 / total)|round|int %}
{% set d3_w = (r_width * d3 / total)|round|int %}
{% set miss_w = (r_width * missed / total)|round|int %}

{% set t_w = 9 %}

{% set d1_x = r_x_start %}
{% set d2_x = d1_x + d1_w %}
{% set d3_x = d2_x + d2_w %}

{% set d1_t_x = (d1_x + d1_w/2)|round|int %}
{% set d2_t_x = (d2_x + d2_w/2)|round|int %}
{% set d3_t_x = (d3_x + d3_w/2)|round|int %}
{% set missed_t_x = (d3_x + d3_w + miss_w/2)|round|int %}

{# {{d1_x}}
{{d1_w}}
{{d2_x}}
{{d2_w}}
{{d3_x}}
{{d3_w}} #}
{# {{d1_t_x}}
{{d2_t_x}}
{{d3_t_x}}
{{missed_t_x}} #}

<svg xmlns="http://www.w3.org/2000/svg" width="299" height="33" viewBox="0 0 299 33">
    <defs>
        <filter id="prefix__Rectangle_{{name}}" width="283" height="33" x="0" y="0" filterUnits="userSpaceOnUse">
            <feOffset dy="3"/>
            <feGaussianBlur result="blur" stdDeviation="3"/>
            <feFlood flood-opacity=".161"/>
            <feComposite in2="blur" operator="in"/>
            <feComposite in="SourceGraphic"/>
        </filter>
        <style>
            .stats__cls-2{fill:#b0b6b6} .stats__cls-6{fill:#f7f7f7;font-size:12px;font-weight:600}
        </style>
    </defs>
    <g id="prefix__napredak_{{name}}" transform="translate(-26 -145)">
        <g id="prefix__progres_bar_{{name}}" data-name="progres bar" style="opacity:.304">
            <g transform="translate(2 14.701) translate(24 130.3)">
                <path id="prefix__Rectangle_103-2" d="M0 0H{{r_width}}V{{r_height}}H0z" class="stats__cls-2 sh_chalk_font" 
                      data-name="Rectangle 103" transform="translate(9 6)"/>
            </g>
        </g>
        <path id="rect_1_{{name}}" d="M0 0H{{d1_w+1}}V{{r_height}}H0z" data-name="Rectangle 107" 
              transform="translate({{d1_x}} {{r_y_start}})" style="fill:#f3b051"/>
        <path id="rect_2_{{name}}" d="M0 0H{{d2_w+1}}V{{r_height}}H0z" data-name="Rectangle 108" 
              transform="translate({{d2_x}} {{r_y_start}})" style="fill:#6ab288"/>
        <path id="rect_2_{{name}}" d="M0 0H{{d3_w}}V{{r_height}}H0z" data-name="Rectangle 109" 
              transform="translate({{d3_x}} {{r_y_start}})" style="fill:#489cba"/>
        {% if t_w < d1_w %}
          <text id="text_1_{{name}}" class="stats__cls-6 sh_lato_font" data-name="d1" transform="translate({{d1_t_x}} {{t_y_start}})">
              <tspan x="-3.48" y="0">{{d1}}</tspan>
          </text>
        {% endif %}
        {% if t_w < d2_w %}
          <text id="text_2_{{name}}" class="stats__cls-6 sh_lato_font" data-name="d2" transform="translate({{d2_t_x}} {{t_y_start}})">
              <tspan x="-6.96" y="0">{{d2}}</tspan>
          </text>
        {% endif %}
        {% if t_w < d3_w %}
          <text id="text_3_{{name}}" class="stats__cls-6 sh_lato_font" data-name="d3" transform="translate({{d3_t_x}} {{t_y_start}})">
              <tspan x="-3.48" y="0">{{d3}}</tspan>
          </text>
        {% endif %}
        {% if t_w < miss_w %}
          <text id="text_miss_{{name}}" data-name="missed" transform="translate({{missed_t_x}} {{t_y_start}})" 
                style="fill:#636661;font-size:12px;font-weight:600" class="sh_lato_font">
              <tspan x="-6.96" y="0">{{missed}}</tspan>
          </text>
        {% endif %}
        <g id="prefix__Icon_awesome-medal_{{name}}" data-name="Icon awesome-medal" transform="translate(300 145)">
            <path id="prefix__Icon_awesome-medal-2_{{name}}" 
