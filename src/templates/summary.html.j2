        </div>

        <div style="display:inline-block; width: 80%; text-align: center; margin: 10px 0 30px 0;">
          {{ elements.stats("type_bar", template_params["type_bar"]["star1"], template_params["type_bar"]["star2"], 
                                  template_params["type_bar"]["star3"], template_params["type_bar"]["missed"], 190) }} 
        </div>


