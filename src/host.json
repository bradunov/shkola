{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  },
  "extensions": {
    "http": {
      "routePrefix" : ""
    }
  },
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Information",
      "Host.Results": "Information",
      "Host.Aggregator": "Information",
      "Microsoft.AspNetCore.Mvc.StatusCodeResult": "Error",
      "Host": "Information", 
      "Function.main": "Debug",
      "Function.edit": "Debug",
      "Function.item": "Error"
    }
  } 
}